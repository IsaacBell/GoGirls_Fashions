<h1>All products</h1>
<ul>
<% @products.each do |p| %>
  <div class="col-md-4">
    <li> <%= p.name %> </li>
    <%= link_to "Edit", edit_product_path(p.id)%>
    <%= link_to "Delete", product_path(p.id),
                                  :method=>"delete", :data=>{:confirm=>"Are you sure?"} %>

  </div>
<% end %>
</ul>
